   35      if ( array_key_exists( $icon, $arr ) ) {
   36:       $repl = '<svg class="tom-icon" width="1em" height="1em" aria-hidden="true" role="img" focusable="false" ';
   37:       $svg  = preg_replace( '/^<svg /', $repl, trim( $arr[ $icon ] ) ); // Add extra attributes to SVG code.
   38:       $svg  = preg_replace( "/([\n\t]+)/", ' ', $svg ); // Remove newlines & tabs.
   39:       $svg  = preg_replace( '/>\s*</', '><', $svg ); // Remove white space between SVG tags.
   40:       return $svg;
   41      }
   42      return null;
